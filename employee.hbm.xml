<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.java.hibernate.Employee" table="employee1">
		<id column="emp_id" name="empId" type="int" />
		<property column="emp_name" name="name" type="string" />
		<property column="emp_salary" name="empSalary" type="float" />
		<property column="emp_address" name="empAddress" type="string" />
	</class>
</hibernate-mapping>

